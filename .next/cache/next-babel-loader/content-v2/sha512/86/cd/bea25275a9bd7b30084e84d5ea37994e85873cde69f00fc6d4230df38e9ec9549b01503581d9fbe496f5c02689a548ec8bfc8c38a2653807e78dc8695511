{"ast":null,"code":"import axios from \"axios\";\nimport { useEffect, useState } from \"react\";\nexport default function useAsyncLoader(_ref) {\n  var url = _ref.url,\n      _ref$method = _ref.method,\n      method = _ref$method === void 0 ? \"get\" : _ref$method,\n      params = _ref.params,\n      data = _ref.data;\n\n  var _useState = useState({\n    isLoading: true,\n    notFound: false,\n    data: null,\n    error: null\n  }),\n      state = _useState[0],\n      setState = _useState[1];\n\n  useEffect(function () {\n    axios[method](url, {\n      data: data,\n      params: params\n    }).then(function (response) {\n      setState({\n        isLoading: false,\n        notFound: false,\n        data: response.data\n      });\n    }).catch(function (error) {\n      setState({\n        isLoading: false,\n        notFound: error.response && error.response.status === 404,\n        data: null,\n        error: error\n      });\n    });\n  }, [url, method, params, data]);\n  return state;\n}","map":null,"metadata":{},"sourceType":"module"}